    "version": "0.2.0",
    "configurations": [
        {
            "name": "Display Video",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/show_video.py",
            "console": "integratedTerminal",
            "args": [
                "--input", "C:/Temp/data/cam_9.mp4",
                // "--begin", "150",
                // "--end",  "300",
            ]
        },
        {
            "name": "Detect Video",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/detect_video.py",
            "console": "integratedTerminal",
            "args": [
                "--detector", "yolov4",
                "--input",  "C:/Temp/data/cam_9.mp4",
                "--output", "C:/Temp/cam_9_det.txt",
                // "--fps", "30",
                "--show_progress",
                // "--show"
            ]
        },
        {
            "name": "Track Video",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/track_video.py",
            "console": "integratedTerminal",
            "args": [
                // "--detector", "yolov4",
                "--detector", "file:C:/Temp/data/cam_9_det.txt",
                "--input",  "C:/Temp/data/cam_9.mp4",
                "--output", "C:/Temp/cam_9_track.txt",
                "--max_age", "20",
                // "--show",
                "--show_progress",
            ]
        },
        {
            "name": "Replay tracks",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/replay_tracks.py",
            "console": "integratedTerminal",
            "args": [
                "--track_file", "C:/Temp/data/cam_9_track.txt",
                "--video_file", "C:/Temp/data/cam_9.mp4",
                "--show",
                // "--show_progress",
            ]
        },
        {
            "name": "DNA Node",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/dna_node.py",
            "console": "integratedTerminal",
            "args": [
                "--camera_id", "ai_city:9",
                "--track_file", "C:/Temp/data/cam_9_track.txt",
                // "--detector", "file:C:/Temp/data/cam_9_det.txt",
                "--input",  "C:/Temp/data/cam_9.mp4",
                "--max_age", "20",
                "--show",
            ]
        },
        {
            "name": "Replay Trajectory",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/replay_trajectory.py",
            "console": "integratedTerminal",
            "args": [
                "--camera_id", "ai_city:9",
                "--luid", "14",
                "--input", "C:/Temp/data/cam_9.mp4",
            ]
        },
        {
            "name": "Format",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/create_schema.py",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "args": []
        },
    ]
}